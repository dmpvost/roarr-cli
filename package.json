{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "bin": {
    "roarr": "./dist/bin/index.js"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "json5": "^2.1.1",
    "prettyjson": "^1.2.1",
    "roarr": "^2.14.4",
    "searchjs": "^1.0.2",
    "split2": "^3.1.1",
    "through2": "^3.0.1",
    "ulid": "^2.3.0",
    "yargs": "^14.2.0"
  },
  "description": "A CLI program for Roarr logger.",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-flow-strip-types": "^7.6.3",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "ava": "^2.4.0",
    "babel-plugin-istanbul": "^5.2.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-canonical": "^18.1.0",
    "flow-bin": "^0.112.0",
    "flow-copy-source": "^2.0.8",
    "husky": "^3.0.9",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.30"
  },
  "engines": {
    "node": ">=10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "log",
    "logger",
    "json"
  ],
  "name": "@roarr/cli",
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "html",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "silent": true,
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gajus/roarr-cli.git"
  },
  "scripts": {
    "build": "rm -fr ./dist && NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps && flow-copy-source src dist",
    "dev": "NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps --watch",
    "lint": "eslint ./src ./test && flow",
    "test": "NODE_ENV=test ava --serial --verbose"
  },
  "version": "1.0.0"
}
